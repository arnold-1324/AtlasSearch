{
   "action": {
      "default_title": "Open OpenClaw Copilot"
   },
   "background": {
      "service_worker": "background.js"
   },
   "description": "OpenClaw (MoltBot & ClawdBot) in your browser: Chat directly in the side panel, no need to switch IM apps.",
   "host_permissions": [ "http://127.0.0.1/*", "http://localhost/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "OpenClaw Copilot",
   "permissions": [ "sidePanel", "storage", "activeTab", "debugger", "tabs", "notifications", "alarms" ],
   "side_panel": {
      "default_path": "sidepanel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.5"
}
